<?import org.controlsfx.control.CheckComboBox?>

<AnchorPane prefHeight="307.0" prefWidth="285.0" xmlns="http://javafx.com/javafx/8.0.65"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.view.ColorPickDialogController">
    <GridPane layoutX="49.0" layoutY="23.0" prefHeight="370.0" prefWidth="348.0" AnchorPane.bottomAnchor="0.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="23.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="136.0" minWidth="10.0" prefWidth="82.0"/>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="219.0" minWidth="10.0" prefWidth="203.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <padding>
            <Insets left="10.0"/>
        </padding>
        <Label text="Kolor"/>
        <Label text="Finish" GridPane.rowIndex="1"/>
        <ComboBox fx:id="colorComboBox" prefHeight="31.0" prefWidth="186.0" GridPane.columnIndex="1"/>
        <CheckComboBox fx:id="finishBox" prefHeight="31.0" prefWidth="187.0" GridPane.columnIndex="1"
                       GridPane.rowIndex="1"/>
        <ButtonBar prefHeight="40.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
            <buttons>
                <Button defaultButton="true" mnemonicParsing="false" onAction="#handleOk" text="Ok"/>
                <Button cancelButton="true" mnemonicParsing="false" onAction="#handleCancel" text="Cancel"/>
            </buttons>
            <padding>
                <Insets right="10.0"/>
            </padding>
        </ButtonBar>
    </GridPane>
</AnchorPane>
